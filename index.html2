<div><br class="Apple-interchange-newline"><!DOCTYPE html><br><html><br><head><br><meta charset="UTF-8"><br><title>deltarune ost sorter</title><br><link rel="shortcut icon" href="https://sv.tinypic.com?ref=qq68e9"><br><link href='https://fonts.googleapis.com/css?family=Josefin+Slab:600' rel='stylesheet' type='text/css'><br><style><br>body {<br> width: 600px;<br> margin: 0 auto;<br> font-family: 'Times New Roman', serif;<br> background: #fff2fb;<br> color: #000000;<br>}<br>#mainTable {<br> font-size: 19px;<br> font-family: 'Times New Roman', serif;<br> text-align: center;<br> vertical-align: middle;<br> width: 410px;<br> margin-left: auto;<br> margin-right: auto;<br> border-collapse: separate;<br> border-spacing: 10px 5px;<br>}<br>#leftField, #rightField {<br> width: 120px;<br> height: 150px;<br> border: 1px solid #000;<br> cursor: pointer;<br> background:#ffffff;<br>}<br>.middleField {<br> width: 120px;<br> height: 70px;<br> border: 1px solid #000;<br> cursor: pointer;<br> background: #ffffff;<br>}<br>.instructions {<br> text-align: center;<br> margin-top: 30px;<br>}<br></style><br></head><br><body><br><p class="instructions"><br><b>deltarune chapters 1-4 OST sorter</b><br><br><br>pick your favorite song from the soundtrack to get an accurate ranking of your favorite tracks<br><br>note: hitting 'no opinion' or 'I like both' frequently will negatively affect your results<br><br></p><br><br><table id="mainTable"><br> <tr><br> <td id="roundNumber" colspan="3"><b>round #1<br>0% sorted.</b></td><br> </tr><br> <tr><br> <td id="leftField" onclick="if(finishFlag==0) sortList(-1);"></td><br> <td class="middleField" onclick="if(finishFlag==0) sortList(0);">I like both</td><br> <td id="rightField" onclick="if(finishFlag==0) sortList(1);"></td><br> </tr><br> <tr><br> <td colspan="3" class="middleField" onclick="if(finishFlag==0) sortList(0);">no opinion</td><br> </tr><br></table><br><br><div id="resultField" style="text-align:center;"></div><br><br><script><br>let namMember = ["ANOTHER HIM", "Beginning", "School", "Susie", "The Door", "The Chase", "The Legend",<br><br>"Lancer",<br><br>"Rude Buster",<br><br>"Empty Town",<br><br>"Weird Birds",<br><br>"Field of Hopes and Dreams",<br><br>"Lantern",<br><br>"Checker Dance",<br><br>"Quiet Autumn",<br><br>"Scarlet Forest",<br><br>"Thrash Machine",<br><br>"Vs. Lancer",<br><br>"Vs. Susie",<br><br>"Card Castle",<br><br>"Rouxls Kaard",<br><br>"April 2012",<br><br>"Hip Shop",<br><br>"Gallery",<br><br>"Chaos King",<br><br>"Darkness Falls",<br><br>"The Circus",<br><br>"THE WORLD REVOLVING",<br><br>"Friendship",<br><br>"THE HOLY",<br><br>"A Town Called Hometown",<br><br>"You Can Always Come Home",<br><br>"Don't Forget",<br><br>"Before the Story",<br><br>"Faint Glow",<br><br>"Girl Next Door",<br><br>"My Castle Town",<br><br>"Queen",<br><br>"A CYBER'S WORLD?",<br><br>"A Simple Diversion",<br><br>"Cyber Battle",<br><br>"Berdly",<br><br>"Smart Race",<br><br>"Faint Courage",<br><br>"WELCOME TO THE CITY",<br><br>"Cool Mixtape",<br><br>"HEY EVERY !",<br><br>"Spamton",<br><br>"NOW'S YOUR CHANCE TO BE A",<br><br>"Elegant Entrance",<br><br>"Bluebird of Misfortune",<br><br>"Pandora Palace",<br><br>"Acid Tunnel of Love",<br><br>"It's Pronounced Rules",<br><br>"Lost Girl",<br><br>"Ferris Wheel",<br><br>"Attack of the Killer Queen",<br><br>"Giga Size",<br><br>"Knock You Down !!",<br><br>"The Dark Truth",<br><br>"Deal Gone Wrong",<br><br>"BIG SHOT",<br><br>"Dialtone",<br><br>"Chill Jailbreak Alarm to Study and Relax to",<br><br>"Until Next Time",<br><br>"Berdly Rejected Concept",<br><br>"Flashback",<br><br>"Feature Presentation",<br><br>"And Now For Today's Sponsors...!",<br><br>"MIKE, the BOARD, please!",<br><br>"Sandy Board",<br><br>"Adventure Board",<br><br>"Query?",<br><br>"Quiz!",<br><br>"Dig! Dig! To The Center of the Earth!",<br><br>"Pushing Buddies",<br><br>"Ruder Buster",<br><br>"Physical Challenge",<br><br>"Board Clear!",<br><br>"Welcome to the Green Room",<br><br>"Vapor Buster",<br><br>"Paradise, Paradise",<br><br>"Raft Ride",<br><br>"SOUTH OF THE BORDER!!",<br><br>"Sound Check",<br><br>"Raise Up Your Bat",<br><br>"KING OF ROLYPOLY",<br><br>"Glowing Snow",<br><br>"Big City Board",<br><br>"Doom Board",<br><br>"Metaphysical Challenge",<br><br>"TV WORLD",<br><br>"It's TV Time!",<br><br>"Black Knife",<br><br>"Dump",<br><br>"SWORD",<br><br>"NORTHERNLIGHT",<br><br>"GLACEIR",<br><br>"BIT ROOTS",<br><br>"ERAM",<br><br>"BURNING EYES",<br><br>"Another day in hometown",<br><br>"Friends",<br><br>"Castle Funk",<br><br>"Gingerbread House",<br><br>"Dark Sanctuary",<br><br>"From Now On",<br><br>"Gyaa Ha ha!",<br><br>"A DARK ZONE",<br><br>"Ever Higher",<br><br>"Wise words",<br><br>"Piano that may not be played that well",<br><br>"Hammer of Justice",<br><br>"The Second Sanctuary",<br><br>"Ripple",<br><br>"13am",<br><br>"12am",<br><br>"The Third Sanctuary",<br><br>"Dark Place",<br><br>"Crumbling Tower",<br><br>"SPAWN",<br><br>"GUARDIAN",<br><br>"Need a hand!?",<br><br>"The place where it rained",<br><br>"The Ol' Jitterbug",<br><br>"Neverending Night",<br><br>"With Hope Crossed On Our Hearts",<br><br> "Catswing", "Air Waves", "Egg room"];<br>let lstMember = [], parent = [], equal = [], rec = [];<br>let cmp1, cmp2, head1, head2, nrec;<br>let numQuestion, totalSize, finishSize, finishFlag;<br><br>function initList() {<br> let n = 0;<br> lstMember[n] = [];<br> for (let i = 0; i < namMember.length; i++) lstMember[n][i] = i;<br> parent[n] = -1;<br> totalSize = 0;<br> n++;<br><br> for (let i = 0; i < lstMember.length; i++) {<br> if (lstMember[i].length >= 2) {<br> let mid = Math.ceil(lstMember[i].length / 2);<br> lstMember[n] = lstMember[i].slice(0, mid);<br> totalSize += lstMember[n].length;<br> parent[n++] = i;<br> lstMember[n] = lstMember[i].slice(mid);<br> totalSize += lstMember[n].length;<br> parent[n++] = i;<br> }<br> }<br> for (let i = 0; i < namMember.length; i++) rec[i] = 0;<br> nrec = 0;<br> for (let i = 0; i <= namMember.length; i++) equal[i] = -1;<br> cmp1 = lstMember.length - 2;<br> cmp2 = lstMember.length - 1;<br> head1 = 0;<br> head2 = 0;<br> numQuestion = 1;<br> finishSize = 0;<br> finishFlag = 0;<br>}<br><br>function sortList(flag) {<br> let i;<br> if (flag < 0) {<br> rec[nrec++] = lstMember[cmp1][head1++]; finishSize++;<br> while (equal[rec[nrec - 1]] != -1) {<br> rec[nrec++] = lstMember[cmp1][head1++]; finishSize++;<br> }<br> } else if (flag > 0) {<br> rec[nrec++] = lstMember[cmp2][head2++]; finishSize++;<br> while (equal[rec[nrec - 1]] != -1) {<br> rec[nrec++] = lstMember[cmp2][head2++]; finishSize++;<br> }<br> } else {<br> rec[nrec++] = lstMember[cmp1][head1++]; finishSize++;<br> while (equal[rec[nrec - 1]] != -1) {<br> rec[nrec++] = lstMember[cmp1][head1++]; finishSize++;<br> }<br> equal[rec[nrec - 1]] = lstMember[cmp2][head2];<br> rec[nrec++] = lstMember[cmp2][head2++]; finishSize++;<br> while (equal[rec[nrec - 1]] != -1) {<br> rec[nrec++] = lstMember[cmp2][head2++]; finishSize++;<br> }<br> }<br><br> if (head1 < lstMember[cmp1].length && head2 == lstMember[cmp2].length) {<br> while (head1 < lstMember[cmp1].length) { rec[nrec++] = lstMember[cmp1][head1++]; finishSize++; }<br> } else if (head1 == lstMember[cmp1].length && head2 < lstMember[cmp2].length) {<br> while (head2 < lstMember[cmp2].length) { rec[nrec++] = lstMember[cmp2][head2++]; finishSize++; }<br> }<br><br> if (head1 == lstMember[cmp1].length && head2 == lstMember[cmp2].length) {<br> for (i = 0; i < lstMember[cmp1].length + lstMember[cmp2].length; i++) {<br> lstMember[parent[cmp1]][i] = rec[i];<br> }<br> lstMember.pop();<br> lstMember.pop();<br> cmp1 -= 2;<br> cmp2 -= 2;<br> head1 = 0;<br> head2 = 0;<br> for (i = 0; i < namMember.length; i++) rec[i] = 0;<br> nrec = 0;<br> }<br><br> if (cmp1 < 0) {<br> document.getElementById("roundNumber").innerHTML = "round #" + (numQuestion - 1) + "<br>" + Math.floor(finishSize * 100 / totalSize) + "% sorted.";<br> showResult();<br> finishFlag = 1;<br> } else {<br> showImage();<br> }<br>}<br><br>function showResult() {<br> let ranking = 1;<br> let sameRank = 1;<br> let str = "<table style=\"width:200px; font-size:18px; line-height:120%; margin:auto; border:1px solid #000; border-collapse:collapse\">";<br> str += "<tr><td style='background:#f0afdb;color:#fff;text-align:center;'>rank</td><td style='background:#f0afdb;color:#fff;text-align:center;'>options</td></tr>";<br> for (let i = 0; i < namMember.length; i++) {<br> str += "<tr><td style='border:1px solid #000;text-align:center;'>" + ranking + "</td><td style='border:1px solid #000;'>" + namMember[lstMember[0][i]] + "</td></tr>";<br> if (i < namMember.length - 1) {<br> if (equal[lstMember[0][i]] == lstMember[0][i + 1]) sameRank++;<br> else { ranking += sameRank; sameRank = 1; }<br> }<br> }<br> str += "</table>";<br> document.getElementById("resultField").innerHTML = str;<br>}<br><br>function showImage() {<br> document.getElementById("roundNumber").innerHTML = "round #" + numQuestion + "<br>" + Math.floor(finishSize * 100 / totalSize) + "% sorted.";<br> document.getElementById("leftField").innerHTML = namMember[lstMember[cmp1][head1]];<br> document.getElementById("rightField").innerHTML = namMember[lstMember[cmp2][head2]];<br> numQuestion++;<br>}<br><br>window.onload = () => {<br> initList();<br> showImage();<br>};<br></script><br><div style="text-align:center; margin-bottom:160px; font-size:14px; color:#555;"><br> <p>modified from <a href="https://pastebin.com/UQChZUwr" target="_blank">biassorter's</a> sorter script </p><br></div><br></body><br></html><br></body><br></html></div>

